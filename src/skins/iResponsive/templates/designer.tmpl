                <div class="toolbox--image-selection--gallery active">
                    <div id="gallery-selector" class="toolbox--image-selection--gallery--selector aam-dropdown"></div>
                    <div class="toolbox--image-selection--gallery--container"></div>

                    <div class="controls--container">
                        <div class="range--container">

                            <button id="btn-scale-decrease" class="range--container--button" title="" aria-label="" data-ref="decrease">
                                <i class="minus"></i>
                            </button>
                            <input type="range" id="scale-control" min="10" max="200" name="scale-range" class="range-control">
                            <button id="btn-scale-increase" class="range--container--button" title="" aria-label="" data-ref="increase">
                                <i class="plus"></i>
                            </button>

                        </div>
                        <div class="buttons--container">
                            <button id="btn-rotate-left" class="control-button">
                                <svg><path d="M18.885 3.515c-4.617-4.618-12.056-4.676-16.756-.195l-2.129-2.258v7.938h7.484l-2.066-2.191c2.82-2.706 7.297-2.676 10.073.1 4.341 4.341 1.737 12.291-5.491 12.291v4.8c3.708 0 6.614-1.244 8.885-3.515 4.686-4.686 4.686-12.284 0-16.97z"/></svg>
